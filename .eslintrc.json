    "extends": [
        "airbnb",
        "gatsby-standard",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "no-unused-expressions": ["error", {
            "allowTaggedTemplates": true
        }]
    }
}